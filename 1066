num=[]
num.append(int(input()))
num.append(int(input()))
num.append(int(input()))
num.append(int(input()))
num.append(int(input()))
par=0
impar=0
neg=0
pos=0
for n in num:
  if(n % 2 == 0):
    par=par+1
  else:
    impar=impar+1
  if(n > 0):
    pos=pos+1
  else:
    if(n<0):
      neg=neg+1
print(par,"valor(es) par(es)")
print(impar,"valor(es) impar(es)")
print(pos,"valor(es) positivo(s)")
print(neg,"valor(es) negativo(s)")
